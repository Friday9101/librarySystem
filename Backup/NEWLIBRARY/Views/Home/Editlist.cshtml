@model IEnumerable<NEWLIBRARY.Models.BOOKS>

@{
    ViewBag.Title = "Editlist";
    Layout = "~/Views/Sheared/Admin_LayOut.cshtml";
}




<table>
    <tr>
        <th>
            @Html.DisplayName("WHICH BOOK DO YOU WHICH TO EDIT?")
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.BOOK_NAME, "Edit", "Home", new { id=item.ID },null)
        </td>

    </tr>
}

</table>



